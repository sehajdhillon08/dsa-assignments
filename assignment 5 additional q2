#include <bits/stdc++.h>
using namespace std;

struct Node{
    int data;
    Node* next;
    Node(int x){data=x;next=NULL;}
};

Node* reverseK(Node* head, int k){
    Node* curr=head;
    Node* prev=NULL;
    Node* nxt=NULL;
    int count=0;

    while(curr && count<k){
        nxt=curr->next;
        curr->next=prev;
        prev=curr;
        curr=nxt;
        count++;
    }
    if(nxt) head->next=reverseK(nxt,k);
    return prev;
}

void printList(Node* head){
    while(head){
        cout<<head->data<<" ";
        head=head->next;
    }
}

int main(){
    Node* head=new Node(1);
    head->next=new Node(2);
    head->next->next=new Node(3);
    head->next->next->next=new Node(4);
    head->next->next->next->next=new Node(5);

    int k=2;

    head=reverseK(head,k);
    printList(head);
}
