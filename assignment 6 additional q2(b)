#include <bits/stdc++.h>
using namespace std;

int parity(int x){
    int c=0;
    while(x){c+=x&1; x>>=1;}
    return c;
}

struct Node{
    int data;
    Node* prev;
    Node* next;
    Node(int x){data=x;prev=next=NULL;}
};
Node* head=NULL;

void insert(int x){
    Node* n=new Node(x);
    if(!head){head=n;return;}
    Node* t=head;
    while(t->next) t=t->next;
    t->next=n;
    n->prev=t;
}

void removeEvenParity(){
    while(head && parity(head->data)%2==0){
        Node* t=head;
        head=head->next;
        if(head) head->prev=NULL;
        delete t;
    }
    Node* t=head;
    while(t && t->next){
        if(parity(t->next->data)%2==0){
            Node* d=t->next;
            t->next=d->next;
            if(d->next) d->next->prev=t;
            delete d;
        }else t=t->next;
    }
}

void print(){
    Node* t=head;
    while(t){
        cout<<t->data;
        if(t->next) cout<<" <=> ";
        t=t->next;
    }
}

int main(){
    int x;
    while(cin>>x) insert(x);
    removeEvenParity();
    print();
}
