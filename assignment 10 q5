#include <bits/stdc++.h>
using namespace std;

struct Node{
    int data;
    Node* next;
    Node(int x){data=x;next=NULL;}
};

int main(){
    int n; cin>>n;
    vector<int>a(n);
    for(int &x:a) cin>>x;

    vector<Node*> nodes;
    for(int x:a) nodes.push_back(new Node(x));

    for(int i=0;i<n-1;i++) nodes[i]->next=nodes[i+1];
    int loopIndex; cin>>loopIndex;
    if(loopIndex!=-1) nodes[n-1]->next=nodes[loopIndex];

    unordered_set<Node*> s;
    Node* cur=nodes[0];
    while(cur){
        if(s.count(cur)){
            cout<<"true";
            return 0;
        }
        s.insert(cur);
        cur=cur->next;
    }
    cout<<"false";
}
