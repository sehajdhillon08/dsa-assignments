#include <bits/stdc++.h>
using namespace std;

struct Node{
    int data;
    Node* next;
    Node(int x){data=x;next=NULL;}
};
Node* head=NULL;

void insert(int x){
    Node* n=new Node(x);
    if(!head){head=n;return;}
    Node* t=head;
    while(t->next)t=t->next;
    t->next=n;
}

int main(){
    int x;
    while(cin>>x) insert(x);
    Node* t=head;
    while(t && t->next && t->next!=head) t=t->next;
    if(t && t->next==head) cout<<"True";
    else cout<<"False";
}
