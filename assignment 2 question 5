#include <stdio.h> // part a
int main() {
    int n,i,j;
    scanf("%d",&n);
    int a[n][n];
    for(i=0;i<n;i++)
        for(j=0;j<n;j++) {
            if(i==j) a[i][j]=1;
            else a[i][j]=0;
        }
    for(i=0;i<n;i++) {
        for(j=0;j<n;j++) printf("%d ",a[i][j]);
        printf("\n");
    }
}
#include <stdio.h>   // part b
int main() {
    int n,i,j;
    scanf("%d",&n);
    int a[n][n];
    for(i=0;i<n;i++) {
        for(j=0;j<n;j++) {
            if(i<=j) a[i][j]=1;
            else a[i][j]=0;
        }
    }
    for(i=0;i<n;i++) {
        for(j=0;j<n;j++) printf("%d ",a[i][j]);
        printf("\n");
    }
}
#include <stdio.h>   // part c
int main() {
    int n,i,j;
    scanf("%d",&n);
    int a[n][n];
    for(i=0;i<n;i++) {
        for(j=0;j<n;j++) {
            if(i>=j) a[i][j]=1;
            else a[i][j]=0;
        }
    }
    for(i=0;i<n;i++) {
        for(j=0;j<n;j++) printf("%d ",a[i][j]);
        printf("\n");
    }
}#include <stdio.h> // part d
int main() {
    int n,i,j;
    scanf("%d",&n);
    int a[n][n];
    for(i=0;i<n;i++) {
        for(j=0;j<n;j++) {
            if(i==j) a[i][j]=1;
            else a[i][j]=2;
        }
    }
    for(i=0;i<n;i++) {
        for(j=0;j<n;j++) printf("%d ",a[i][j]);
        printf("\n");
    }
}

