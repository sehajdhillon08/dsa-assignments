#include <bits/stdc++.h>
using namespace std;

struct Node{
    int data;
    Node* left;
    Node* right;
    Node(int x){data=x;left=right=NULL;}
};

Node* build(){
    int x; cin>>x;
    if(x==-1) return NULL;
    Node* r=new Node(x);
    r->left=build();
    r->right=build();
    return r;
}

void preorder(Node* r){
    if(!r) return;
    cout<<r->data<<" ";
    preorder(r->left);
    preorder(r->right);
}

void inorder(Node* r){
    if(!r) return;
    inorder(r->left);
    cout<<r->data<<" ";
    inorder(r->right);
}

void postorder(Node* r){
    if(!r) return;
    postorder(r->left);
    postorder(r->right);
    cout<<r->data<<" ";
}

int main(){
    Node* root=build();
    preorder(root); cout<<endl;
    inorder(root); cout<<endl;
    postorder(root);
}
