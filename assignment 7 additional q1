#include <bits/stdc++.h>
using namespace std;

int main(){
    int n; cin>>n;
    vector<int>a(n);
    for(int i=0;i<n;i++) cin>>a[i];

    int mx=*max_element(a.begin(),a.end());
    vector<int>cnt(mx+1,0);

    for(int x:a) cnt[x]++;
    for(int i=1;i<=mx;i++) cnt[i]+=cnt[i-1];

    vector<int>out(n);
    for(int i=n-1;i>=0;i--){
        out[cnt[a[i]]-1]=a[i];
        cnt[a[i]]--;
    }

    for(int x:out) cout<<x<<" ";
}
