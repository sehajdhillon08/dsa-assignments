#include <stdio.h>

int main() {
    int arr[] = {1, 2, 2, 3, 4, 4, 5};
    int n = 7, k = 0;
    for (int i = 0; i < n; i++) {
        int j;
        for (j = 0; j < k; j++) if (arr[i] == arr[j]) break;
        if (j == k) arr[k++] = arr[i];
    }
    for (int i = 0; i < k; i++) printf("%d ", arr[i]);
    return 0;
}
