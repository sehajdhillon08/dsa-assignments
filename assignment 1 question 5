#include <stdio.h>
int main() {
    int a[10][10], row, col, i, j, sum;

    scanf("%d%d",&row,&col);
    for(i=0;i<row;i++)
        for(j=0;j<col;j++)
            scanf("%d",&a[i][j]);

    for(i=0;i<row;i++) {
        sum = 0;
        for(j=0;j<col;j++) sum += a[i][j];
        printf("%d\n", sum);
    }

    for(j=0;j<col;j++) {
        sum = 0;
        for(i=0;i<row;i++) sum += a[i][j];
        printf("%d\n", sum);
    }

    return 0;
}
