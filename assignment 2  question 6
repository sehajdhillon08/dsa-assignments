#include <stdio.h> // transpose
int main() {
    int n,m,i,j;
    scanf("%d%d",&n,&m);
    int a[n][m],t[m][n];
    for(i=0;i<n;i++)
        for(j=0;j<m;j++)
            scanf("%d",&a[i][j]);
    for(i=0;i<n;i++)
        for(j=0;j<m;j++)
            t[j][i]=a[i][j];
    for(i=0;i<m;i++) {
        for(j=0;j<n;j++)
            printf("%d ",t[i][j]);
        printf("\n");
    }
}
#include <stdio.h>  // addition
int main() {
    int n,m,i,j;
    scanf("%d%d",&n,&m);
    int a[n][m],b[n][m],c[n][m];
    for(i=0;i<n;i++)
        for(j=0;j<m;j++)
            scanf("%d",&a[i][j]);
    for(i=0;i<n;i++)
        for(j=0;j<m;j++)
            scanf("%d",&b[i][j]);
    for(i=0;i<n;i++) {
        for(j=0;j<m;j++) {
            c[i][j]=a[i][j]+b[i][j];
            printf("%d ",c[i][j]);
        }
        printf("\n");
    }
}
#include <stdio.h>  // multiplication
int main() {
    int n,m,p,q,i,j,k;
    scanf("%d%d",&n,&m);
    scanf("%d%d",&p,&q);
    int a[n][m],b[p][q],c[n][q];
    for(i=0;i<n;i++)
        for(j=0;j<m;j++)
            scanf("%d",&a[i][j]);
    for(i=0;i<p;i++)
        for(j=0;j<q;j++)
            scanf("%d",&b[i][j]);
    for(i=0;i<n;i++)
        for(j=0;j<q;j++) {
            c[i][j]=0;
            for(k=0;k<m;k++)
                c[i][j]+=a[i][k]*b[k][j];
        }
    for(i=0;i<n;i++) {
        for(j=0;j<q;j++)
            printf("%d ",c[i][j]);
        printf("\n");
    }
}



