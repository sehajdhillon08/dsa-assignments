#include <iostream>
#include <queue>
using namespace std;

class Stack {
    queue<int> q;

public:
    void push(int x) {
        int size = q.size();
        q.push(x);

        while (size--) {
            q.push(q.front());
            q.pop();
        }
    }

    void pop() {
        if (q.empty()) {
            cout << "Empty\n";
            return;
        }
        q.pop();
    }

    int top() {
        return q.front();
    }
};

int main() {
    Stack s;
    s.push(5);
    s.push(10);
    s.push(15);
    cout << "Top = " << s.top() << endl;
    s.pop();
    cout << "Top = " << s.top() << endl;
}
