#include <iostream>
using namespace std;

#define MAX 20

int q[MAX], front = -1, rear = -1;

bool isFull() {
    return rear == MAX - 1;
}

bool isEmpty() {
    return front == -1 || front > rear;
}

void enqueue(int x) {
    if (isFull()) {
        cout << "Queue Full\n";
        return;
    }
    if (front == -1) front = 0;
    q[++rear] = x;
}

void dequeue() {
    if (isEmpty()) {
        cout << "Queue Empty\n";
        return;
    }
    cout << "Deleted: " << q[front++] << endl;
}

void peek() {
    if (isEmpty()) cout << "Empty\n";
    else cout << "Peek: " << q[front] << endl;
}

void display() {
    if (isEmpty()) {
        cout << "Empty\n";
        return;
    }
    for (int i = front; i <= rear; i++)
        cout << q[i] << " ";
    cout << endl;
}

int main() {
    int ch, x;
    while (true) {
        cout << "\n1.Enqueue 2.Dequeue 3.Peek 4.Display 5.Exit\n";
        cin >> ch;

        switch(ch) {
            case 1: cin >> x; enqueue(x); break;
            case 2: dequeue(); break;
            case 3: peek(); break;
            case 4: display(); break;
            case 5: return 0;
        }
    }
}
