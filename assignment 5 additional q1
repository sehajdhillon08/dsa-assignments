#include <bits/stdc++.h>
using namespace std;

struct Node{
    int data;
    Node* next;
    Node(int x){data=x;next=NULL;}
};

int getLength(Node* head){
    int cnt=0;
    while(head){cnt++; head=head->next;}
    return cnt;
}

Node* getIntersection(Node* h1, Node* h2){
    int n1=getLength(h1), n2=getLength(h2);
    int d=abs(n1-n2);

    if(n1>n2) while(d--) h1=h1->next;
    else while(d--) h2=h2->next;

    while(h1 && h2){
        if(h1==h2) return h1;
        h1=h1->next;
        h2=h2->next;
    }
    return NULL;
}

int main(){
    Node* head1=new Node(1);
    head1->next=new Node(2);
    Node* common=new Node(3);
    head1->next->next=common;
    common->next=new Node(4);

    Node* head2=new Node(9);
    head2->next=common;

    Node* ans=getIntersection(head1,head2);
    if(ans) cout<<ans->data;
    else cout<<"No intersection";
}
