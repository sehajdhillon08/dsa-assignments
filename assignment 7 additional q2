#include <bits/stdc++.h>
using namespace std;

int getMax(vector<int>&a){
    int m=a[0];
    for(int x:a) m=max(m,x);
    return m;
}

void countSort(vector<int>&a,int exp){
    int n=a.size();
    vector<int>out(n),cnt(10,0);

    for(int x:a) cnt[(x/exp)%10]++;
    for(int i=1;i<10;i++) cnt[i]+=cnt[i-1];

    for(int i=n-1;i>=0;i--){
        int d=(a[i]/exp)%10;
        out[cnt[d]-1]=a[i];
        cnt[d]--;
    }
    a=out;
}

int main(){
    int n; cin>>n;
    vector<int>a(n);
    for(int i=0;i<n;i++) cin>>a[i];

    int mx=getMax(a);
    for(int exp=1;mx/exp>0;exp*=10)
        countSort(a,exp);

    for(int x:a) cout<<x<<" ";
}
