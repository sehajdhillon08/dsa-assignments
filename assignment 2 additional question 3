#include <stdio.h>

int findMissing(int arr[], int n) {
    int l = 0, r = n - 1;
    while (l <= r) {
        int mid = (l + r) / 2;
        if (arr[mid] != mid + 1) r = mid - 1;
        else l = mid + 1;
    }
    return l + 1;
}

int main() {
    int arr[] = {1, 2, 3, 5, 6, 7};
    int n = 6;
    printf("Missing number is %d", findMissing(arr, n));
    return 0;
}
