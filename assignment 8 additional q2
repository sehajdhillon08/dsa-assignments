#include <bits/stdc++.h>
using namespace std;

struct Node{
    int val;
    Node *left,*right;
    Node(int x){val=x;left=right=NULL;}
};

vector<Node*> build(int l,int r){
    if(l>r) return {NULL};
    vector<Node*> ans;
    for(int i=l;i<=r;i++){
        auto L=build(l,i-1);
        auto R=build(i+1,r);
        for(auto a:L) for(auto b:R){
            Node* n=new Node(i);
            n->left=a;
            n->right=b;
            ans.push_back(n);
        }
    }
    return ans;
}

void preorder(Node* r){
    if(!r){cout<<"-1 "; return;}
    cout<<r->val<<" ";
    preorder(r->left);
    preorder(r->right);
}

int main(){
    int n; cin>>n;
    auto res=build(1,n);
    for(auto t:res){
        preorder(t);
        cout<<endl;
    }
}
