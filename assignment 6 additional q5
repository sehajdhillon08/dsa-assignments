#include <bits/stdc++.h>
using namespace std;

struct Node{
    int data;
    Node *next,*prev,*up,*down;
    Node(int x){data=x;next=prev=up=down=NULL;}
};

Node* buildGrid(vector<vector<int>> &a){
    int n=a.size(), m=a[0].size();
    vector<vector<Node*>> g(n, vector<Node*>(m));

    for(int i=0;i<n;i++)
        for(int j=0;j<m;j++)
            g[i][j]=new Node(a[i][j]);

    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            if(j+1<m) g[i][j]->next = g[i][j+1];
            if(j-1>=0) g[i][j]->prev = g[i][j-1];
            if(i+1<n) g[i][j]->down = g[i+1][j];
            if(i-1>=0) g[i][j]->up = g[i-1][j];
        }
    }
    return g[0][0];
}

void printGrid(Node* head,int n,int m){
    Node* row=head;
    for(int i=0;i<n;i++){
        Node* col=row;
        for(int j=0;j<m;j++){
            cout<<col->data<<" ";
            col=col->next;
        }
        cout<<endl;
        row=row->down;
    }
}

int main(){
    int n,m;
    cin>>n>>m;
    vector<vector<int>> a(n, vector<int>(m));
    for(int i=0;i<n;i++)
        for(int j=0;j<m;j++)
            cin>>a[i][j];

    Node* head=buildGrid(a);
    printGrid(head,n,m);
}
