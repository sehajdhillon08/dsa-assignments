#include <bits/stdc++.h>
using namespace std;

int main(){
    int n,target; cin>>n>>target;
    vector<int>a(n);
    for(int &x:a) cin>>x;

    unordered_set<int>s;
    for(int x:a){
        if(s.count(target-x)){
            cout<<"true";
            return 0;
        }
        s.insert(x);
    }
    cout<<"false";
}
