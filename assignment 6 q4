#include <bits/stdc++.h>
using namespace std;

struct Node{
    char data;
    Node* next;
    Node* prev;
    Node(char x){data=x;next=prev=NULL;}
};
Node* head=NULL;

void insert(char x){
    Node* n=new Node(x);
    if(!head){head=n;return;}
    Node* t=head;
    while(t->next)t=t->next;
    t->next=n;
    n->prev=t;
}

int main(){
    char c;
    while(cin>>c) insert(c);
    Node* l=head;
    Node* r=head;
    while(r->next) r=r->next;
    while(l!=r && r->next!=l){
        if(l->data!=r->data){cout<<"False";return 0;}
        l=l->next;
        r=r->prev;
    }
    cout<<"True";
}
