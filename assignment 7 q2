#include <bits/stdc++.h>
using namespace std;

int main(){
    int n; cin>>n;
    vector<int>a(n);
    for(int i=0;i<n;i++) cin>>a[i];

    int l=0,r=n-1;
    while(l<r){
        int mn=l, mx=r;
        for(int i=l;i<=r;i++){
            if(a[i]<a[mn]) mn=i;
            if(a[i]>a[mx]) mx=i;
        }
        swap(a[l],a[mn]);
        if(mx==l) mx=mn;
        swap(a[r],a[mx]);
        l++; r--;
    }

    for(int x:a) cout<<x<<" ";
}
