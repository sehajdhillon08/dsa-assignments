#include <iostream>
#include <queue>
using namespace std;

int main() {
    queue<int> q, firstHalf;
    int x;

    for (int i = 0; i < 6; i++) {
        cin >> x;
        q.push(x);
    }

    int n = q.size() / 2;

    for (int i = 0; i < n; i++) {
        firstHalf.push(q.front());
        q.pop();
    }

    while (!firstHalf.empty()) {
        cout << firstHalf.front() << " ";
        firstHalf.pop();

        cout << q.front() << " ";
        q.pop();
    }
}
