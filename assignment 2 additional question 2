#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char str[] = "The quick brown fox jumps over the lazy dog";
    int freq[26] = {0}, i;
    for (i = 0; str[i] != '\0'; i++) {
        if (isalpha(str[i])) {
            freq[tolower(str[i]) - 'a'] = 1;
        }
    }
    int pangram = 1;
    for (i = 0; i < 26; i++) {
        if (freq[i] == 0) {
            pangram = 0;
            break;
        }
    }
    if (pangram) printf("Pangram");
    else printf("Not Pangram");
    return 0;
}
